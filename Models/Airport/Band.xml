<?xml version="1.0"?>

<PropertyList>

  <path>Band-ba.ac</path>

  <animation>
    <type>range</type>
    <min-m>0</min-m>
    <max-property>sim/rendering/static-lod/bare</max-property>
  </animation>
	
  <animation>
    <type>material</type>
    <object-name>jour</object-name>
    <emission>
      <red>   0.2 </red>
      <green> 0.2 </green>
      <blue>  0.2 </blue>
    </emission>
  </animation>
	
  <animation>
    <object-name>jour</object-name>
    <enable-hot type="bool">false</enable-hot>
  </animation>

</PropertyList>
