<?xml version="1.0" encoding="UTF-8" ?>
<PropertyList>
    <path>ebbr_terminal_2_jetway.ac</path>

    <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>5000</max-m>
    </animation>

	<!--  Light Cone  -->
	<model>
	<path>Models/Effects/lightCone1000w.xml</path>
		<offsets>
			<x-m>-2.67489</x-m>
			<y-m>-11.4903</y-m>
			<z-m>25.7398</z-m>
			<pitch-deg>45.00</pitch-deg>
			<heading-deg>90</heading-deg>
		</offsets>
	</model>
	
  <!--  night emission  -->
  <animation>
    <type>material</type>
    <object-name>Arm_1</object-name>
    <object-name>Arm_2</object-name>
    <object-name>Arm_3</object-name>
    <object-name>Arm_4</object-name>LightBulb
	<object-name>LightBulb</object-name>
	<object-name>LightPole</object-name>
	<object-name>Box</object-name>
	<object-name>JetwayRail</object-name>
	<object-name>Circle</object-name>
	<object-name>Rot_1</object-name>
	<object-name>Rot_2</object-name>
    <condition>		
      <greater-than>		
	<property>/sim/time/sun-angle-rad</property>
	<value>1.57</value>
      </greater-than>
    </condition>
    <emission>	
      <red>0.2</red>	
      <green>0.2</green>	
      <blue>0.2</blue>
    </emission>
  </animation>
  
</PropertyList>
