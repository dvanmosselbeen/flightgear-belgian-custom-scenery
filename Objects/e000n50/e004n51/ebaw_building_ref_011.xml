	<!--  night emission  -->
	<animation>
		<type>material</type>
		<object-name>Cube.001</object-name>
		<object-name>Cube.011</object-name>
		<object-name>Circle.003</object-name>
		<condition>
			<greater-than>
				<property>/sim/time/sun-angle-rad</property>
				<value>1.49</value>
			</greater-than>
		</condition>
		<emission>
			<red>0.1</red>
			<green>0.1</green>
			<blue>0.1</blue>
