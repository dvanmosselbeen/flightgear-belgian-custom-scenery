<?xml version="1.0"?>

<PropertyList>

    <path>ebbr_terminal_2.ac</path>

    <animation>
        <type>material</type>
        <object-name>Coridor</object-name>
        <object-name>CoridorRoofDetails</object-name>
        <object-name>RoofVentilation</object-name>
        <object-name>RoofVentilation2</object-name>
        <object-name>Terminal2Bloc</object-name>
        <object-name>LittleRoofs</object-name>
        <object-name>RoofDetails</object-name>
        <condition>
            <greater-than>
                <property>/sim/time/sun-angle-rad</property>
                <value>1.57</value>
            </greater-than>
        </condition>
        <texture>ebbr_terminal_2_lightmap.png</texture>
        <emission>
            <red> 0.98</red>
            <green> 0.96</green>
            <blue> 0.78</blue>
        </emission>
    </animation>

    <animation>
        <type>range</type>
        <object-name>RoofDetails</object-name>
        <object-name>CoridorRoofDetails</object-name>
        <min-m>0</min-m>
        <max-m>1000</max-m>
    </animation>

    <animation>
        <type>range</type>
        <object-name>LittleRoofs</object-name>
        <min-m>0</min-m>
        <max-m>3000</max-m>
    </animation>

    <animation>
        <type>range</type>
        <object-name>RoofVentilation</object-name>
        <object-name>RoofVentilation2</object-name>
        <min-m>0</min-m>
        <max-m>5000</max-m>
    </animation>

    <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>15000</max-m>
    </animation>

</PropertyList>
