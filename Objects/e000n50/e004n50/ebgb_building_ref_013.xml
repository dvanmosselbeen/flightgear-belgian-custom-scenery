<?xml version="1.0"?>

<PropertyList>

    <path>ebgb_building_ref_013.ac</path>

     <nasal>
          <load>
               if (!contains(globals, "scenery")) globals["scenery"] = {};
               if (!contains(scenery, "EBGB")) scenery["EBGB"] = {};
               if (!contains(scenery.EBGB, "hangar")) scenery.EBGB["hangar"] =
                 [nil,nil,nil,nil,nil,nil];
               scenery.EBGB.hangar[0] =
                 { door :
                   [aircraft.door.new("/scenery/EBGB/hangar[0]/door[0]", 3*60)]
                 };
          </load>
     </nasal>

    <animation>
        <type>translate</type>
        <object-name>DoorL1</object-name>
        <property>/scenery/EBGB/hangar[0]/door[0]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>-2.921</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
      
    <animation>
        <type>translate</type>
        <object-name>DoorL2</object-name>
        <property>/scenery/EBGB/hangar[0]/door[0]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>-5.842</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>DoorR2</object-name>
        <property>/scenery/EBGB/hangar[0]/door[0]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>5.842</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
      
    <animation>
        <type>translate</type>
        <object-name>DoorR1</object-name>
        <property>/scenery/EBGB/hangar[0]/door[0]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>2.921</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>


    <animation>
        <type>pick</type>
        <object-name>DoorL1</object-name>
        <object-name>DoorL2</object-name>
        <object-name>DoorR1</object-name>
        <object-name>DoorR2</object-name>
        <visible>true</visible>
        <action>
            <name>click</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>scenery.EBGB.hangar[0].door[0].toggle();</script>
            </binding>
        </action>
    </animation>

</PropertyList>

