<PropertyList>

    <path>ebbr_building_ref_078.ac</path>

     <nasal>
          <load>
               if (!contains(globals, "scenery")) globals["scenery"] = {};
               if (!contains(scenery, "EBBR")) scenery["EBBR"] = {};
               if (!contains(scenery.EBBR, "hangar")) scenery.EBBR["hangar"] =
                 [nil,nil,nil,nil,nil,nil];
               scenery.EBBR.hangar[1] =
                 { door :
                    [aircraft.door.new("/scenery/EBBR/hangar[1]/door[0]", 3*60),
                    aircraft.door.new("/scenery/EBBR/hangar[1]/door[1]", 3*60)]
                 };
          </load>
     </nasal>

