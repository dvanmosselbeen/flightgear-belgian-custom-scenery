<?xml version="1.0"?>

<PropertyList>

    <path>ebbr_building_ref_081.ac</path>

     <nasal>
          <load>
               if (!contains(globals, "scenery")) globals["scenery"] = {};
               if (!contains(scenery, "EBBR")) scenery["EBBR"] = {};
               if (!contains(scenery.EBBR, "hangar")) scenery.EBBR["hangar"] =
                 [nil,nil,nil,nil,nil,nil];
               scenery.EBBR.hangar[2] =
                 { door :
                    [aircraft.door.new("/scenery/EBBR/hangar[2]/door[0]", 3*60),
                    aircraft.door.new("/scenery/EBBR/hangar[2]/door[1]", 3*60)]
                 };
          </load>
     </nasal>

    <animation>
        <type>translate</type>
        <object-name>DoorL1</object-name>
        <property>/scenery/EBBR/hangar[2]/door[0]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>-25</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
      
    <animation>
        <type>translate</type>
        <object-name>DoorL2</object-name>
        <property>/scenery/EBBR/hangar[2]/door[0]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>-50</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>DoorL3</object-name>
        <property>/scenery/EBBR/hangar[2]/door[0]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>-75</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
      
    <animation>
        <type>translate</type>
        <object-name>DoorL4</object-name>
        <property>/scenery/EBBR/hangar[2]/door[0]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>-100</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>  

    <animation>
        <type>pick</type>
        <object-name>DoorL1</object-name>
        <object-name>DoorL2</object-name>
        <object-name>DoorL3</object-name>
        <object-name>DoorL4</object-name>
        <visible>true</visible>
        <action>
            <name>click</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>scenery.EBBR.hangar[2].door[0].toggle();</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>DoorR4</object-name>
        <property>/scenery/EBBR/hangar[2]/door[1]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>100</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
      
    <animation>
        <type>translate</type>
        <object-name>DoorR3</object-name>
        <property>/scenery/EBBR/hangar[2]/door[1]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>75</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>translate</type>
        <object-name>DoorR2</object-name>
        <property>/scenery/EBBR/hangar[2]/door[1]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>50</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>
      
    <animation>
        <type>translate</type>
        <object-name>DoorR1</object-name>
        <property>/scenery/EBBR/hangar[2]/door[1]/position-norm</property>
        <interpolation>
            <entry><ind>0</ind><dep>0.0</dep></entry>
            <entry><ind>0.2</ind><dep>25</dep></entry>
        </interpolation>
        <axis>
            <x>1</x>
            <y>0</y>
            <z>0</z>
        </axis>
    </animation>

    <animation>
        <type>pick</type>
        <object-name>DoorR1</object-name>
        <object-name>DoorR2</object-name>
        <object-name>DoorR3</object-name>
        <object-name>DoorR4</object-name>
        <visible>true</visible>
        <action>
            <name>click</name>
            <button>0</button>
            <repeatable>false</repeatable>
            <binding>
                <command>nasal</command>
                <script>scenery.EBBR.hangar[2].door[1].toggle();</script>
            </binding>
        </action>
    </animation>

    <animation>
        <type>range</type>
        <min-m>0</min-m>
        <max-m>15000</max-m>
    </animation>

</PropertyList>

